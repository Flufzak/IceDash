{
  "expo": {
    "name": "icedash",
    "slug": "icedash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon_1.png",
      "resizeMode": "cover",
      "backgroundColor": "#a4e2fd"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon_1.png",
        "backgroundColor": "##a4e2fd"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon_1.png"
    },
    "plugins": ["expo-audio"]
  }
}
